            "expression": "\\b[A-Z]{2}[0-9]{2}(?:[ ]?[0-9]{4}){4}(?:[ ]?[0-9]{1,2})?\\b"
        },
        {
            "label": "REGEX_EMAIL",
            "value": "Regex: Email address",
            "regex_compiled_pos": 2,
            "expression": "\\b[\\w\\-\\.]+@(?:[\\w+\\-]+\\.)+\\w{2,10}\\b"
        },
        {
            "label": "REGEX_IPV4",
            "value": "Regex: IPv4 address",
            "regex_compiled_pos": 3,
            "expression": "\\b(?:(?:25[0-5]|(?:2[0-4]|1\\d|[1-9]|)\\d)\\.?\\b){4}\\b"
        },
        {
            "label": "REGEX_WORDS",
            "value": "Regex: Signal words",
            "regex_compiled_pos": 4,
            "expression": "\b(?:Abmahnung|Bewerbung|Zeugnis|Entwicklungsbericht|Gutachten|Krankmeldung)\b"
        },
        {
            "label": "REGEX_PGPPRV",
            "value": "Regex: private pgp key",

    "regex": [
        {
            "label": "REGEX_RVNR",
            "value": "Regex: Pension Insurance Fond ID",
            "regex_compiled_pos": 0,
            "expression": "\\b[0-8][0-9][0-3][0-9][0-1][0-9]{3}[A-Z][0-9]{3}\\b"
        },
        {
            "label": "REGEX_IBAN",
            "value": "Regex: IBAN",
            "regex_compiled_pos": 1,
            "expression": "\\b[A-Z]{2}[0-9]{2}(?:[ ]?[0-9]{4}){4}(?:[ ]?[0-9]{1,2})?\\b"
        },
        {
            "label": "REGEX_EMAIL",
            "value": "Regex: Email address",
            "regex_compiled_pos": 2,
            "expression": "\\b[\\w\\-\\.]+@(?:[\\w+\\-]+\\.)+\\w{2,10}\\b"
        },
        {
            "label": "REGEX_IPV4",
            "value": "Regex: IPv4 address",
            "regex_compiled_pos": 3,
            "expression": "\\b(?:(?:25[0-5]|(?:2[0-4]|1\\d|[1-9]|)\\d)\\.?\\b){4}\\b"
        },
        {
            "label": "REGEX_WORDS",
            "value": "Regex: Signal words",
            "regex_compiled_pos": 4,
            "expression": "\\b(?:Abmahnung|Bewerbung|Zeugnis|Entwicklungsbericht|Gutachten|Krankmeldung)\\b"
        },
        {
            "label": "REGEX_PGPPRV",
            "value": "Regex: private pgp key",
            "regex_compiled_pos": 5,
            "expression": "(?:BEGIN PGP PRIVATE KEY)"
        },
        {
            "label": "REGEX_PHONE",
            "value": "Regex: Phone Number",
            "regex_compiled_pos": 6,
            "expression": "\\b(?:\\+?[1-9]\\d{1,14}|0[1-9]\\d{1,13})\\b"
        },
        {
            "label": "REGEX_TAX_ID",
            "value": "Regex: Tax ID (Steuer-ID)",
            "regex_compiled_pos": 7,
            "expression": "\\b[0-9]{11}\\b"
        },
        {
            "label": "REGEX_BIC",
            "value": "Regex: BIC (Bank Identifier Code)",
            "regex_compiled_pos": 8,
            "expression": "\\b[A-Z]{4}[A-Z]{2}[A-Z0-9]{2}(?:[A-Z0-9]{3})?\\b"
        },
        {
            "label": "REGEX_POSTAL_CODE",
            "value": "Regex: Postal Code (DE)",
            "regex_compiled_pos": 9,
            "expression": "\\b[0-9]{5}\\b"
        },
        {
            "label": "REGEX_SIGNAL_WORDS_EXTENDED",
            "value": "Regex: Extended Signal Words",
            "regex_compiled_pos": 10,
            "expression": "\\b(?:Diagnose|Therapie|Medikament|Krankheit|Behandlung|Arzt|Klinik|Krankenhaus|Patient|Symptom|Operation|Rezept|Gehalt|Lohn|Einkommen|Vermögen|Schulden|Kredit|Darlehen|Kontostand|Überweisung|Rechnung|Mahnung|Klage|Anwalt|Gericht|Urteil|Vertrag|Vereinbarung|Einverständniserklärung|Datenschutzerklärung|Lebenslauf|CV|Bewerbung|Referenz|Arbeitszeugnis|Qualifikation|Erfahrung|Kompetenz)\\b"
        },
        {
            "label": "REGEX_CREDIT_CARD",
            "value": "Regex: Credit Card Number",
            "regex_compiled_pos": 11,
            "expression": "\\b(?:4[0-9]{12}(?:[0-9]{3})?|5[1-5][0-9]{14}|3[47][0-9]{13}|3[0-9]{13}|6(?:011|5[0-9]{2})[0-9]{12})\\b",
            "validation": "luhn"
        },
        {
            "label": "REGEX_PASSPORT",
            "value": "Regex: Passport Number",
            "regex_compiled_pos": 12,
            "expression": "\\b[A-Z]{1,2}[0-9]{6,9}\\b"
        },
        {
            "label": "REGEX_PERSONALAUSWEIS",
            "value": "Regex: Personalausweis Number (DE)",
            "regex_compiled_pos": 13,
            "expression": "\\b[CFGHJKLMNPRTVWXYZ0-9]{9}\\b"
        },
        {
            "label": "REGEX_SSN_US",
            "value": "Regex: Social Security Number (US)",
            "regex_compiled_pos": 14,
            "expression": "\\b(?!000|666|9[0-9]{2})[0-9]{3}-?(?!00)[0-9]{2}-?(?!0000)[0-9]{4}\\b"
        },
        {
            "label": "REGEX_SSN_AT",
            "value": "Regex: Social Security Number (AT)",
            "regex_compiled_pos": 15,
            "expression": "\\b[0-9]{3}\\.[0-9]{2}\\.[0-9]{4}\\b"
        },
        {
            "label": "REGEX_SSN_CH",
            "value": "Regex: AHV Number (CH)",
            "regex_compiled_pos": 16,
            "expression": "\\b756\\.[0-9]{4}\\.[0-9]{4}\\.[0-9]{2}\\b"
        },
        {
            "label": "REGEX_MRN",
            "value": "Regex: Medical Record Number",
            "regex_compiled_pos": 17,
            "expression": "\\b(?:MRN|Medical Record Number|Patient ID)[\\s:]*[A-Z0-9]{6,12}\\b"
        }
    ],
